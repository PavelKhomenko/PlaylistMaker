<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="320dp"
        android:height="320dp"
        android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/margin_8dp"/>
            <stroke
                android:color="@color/yp_text_gray"
                android:width="1dp"
                android:dashWidth="@dimen/margin_24dp"
                android:dashGap="@dimen/margin_24dp"/>
        </shape>
    </item>

    <item
        android:width="100dp"
        android:height="100dp"
        android:gravity="center"
        android:drawable="@drawable/create_playlist_photo_placeholder">
    </item>

</layer-list>